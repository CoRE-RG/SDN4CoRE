<!--
This is an example file on how to declare remotes for an INetConfApplication.
-->

<server_connections>
	<application client_host="controller" client_app="1">
		<connection local_port="6130" remote_address="switch1" remote_port="830" connect_at="0.12">
			<configure at="0.13" type="lock" module="IEEE8021Qbv" target="candidate">
		 	</configure>
			<configure at="0.15" type="unlock" module="IEEE8021Qbv" target="candidate">
		 	</configure>
		</connection>
	</application>
</server_connections>

