			 	<config port="41" gatecontrollist="o,o,o,o,o,o,o,o:0;C,C,C,C,C,C,C,C:0.00025"/>
		 	</configure>
		</connection>
		<connection local_port="6124" remote_address="switch2" remote_port="830" connect_at="0.12">
			<configure at="0.15" type="edit_config" module="IEEE8021Qbv">
			 	<config port="42" gatecontrollist="o,o,o,o,o,o,o,o:0;C,C,C,C,C,C,C,C:0.00025"/>
			 	<config port="43" gatecontrollist="o,o,o,o,o,o,o,o:0;C,C,C,C,C,C,C,C:0.00025"/>
