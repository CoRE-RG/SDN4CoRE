<server_connections>
	<!-- Add S1 -->
	<application client_host="controller" client_app="0">
		<connection local_port="6101" remote_address="switchleft" remote_port="830">
			<configure type="edit_config" module="Multipart">
				<configure type="edit_config" module="IEEE8021Qbv">
				 	<config port="0" gatecontrollist="o,o,o,o,o,o,o,C:0;C,C,C,C,C,C,C,C:0.000500;C,C,C,C,C,C,C,o:0.000624;o,o,o,o,o,o,o,C:0.000747"/>
			 	</configure>
			 	<configure type="edit_config" module="OpenFlow">
				 	<config operation="ADD_FLOW" tableIndex="0">
						<flowEntry hardTimeout="0" idleTimeout="10" cookie="42" priority="40000" flags="0" >
						    <match in_port="1" dl_dst="0A-00-00-00-00-04" dl_src="0A-00-00-00-00-01" dl_vlan_pcp="7" dl_type="33024" wildcards="3145698" />
						    <instructions>
						        <action_output port="0"/>
						    </instructions>
						</flowEntry>
					</config>
			 	</configure>
		 	</configure>
		</connection>
		<connection local_port="6102" remote_address="switchright" remote_port="830">
			<configure type="edit_config" module="Multipart">
				<configure type="edit_config" module="IEEE8021Qbv">
				 	<config port="2" gatecontrollist="o,o,o,o,o,o,o,C:0;C,C,C,C,C,C,C,C:0.000625;C,C,C,C,C,C,C,o:0.000749;o,o,o,o,o,o,o,C:0.000873"/>
